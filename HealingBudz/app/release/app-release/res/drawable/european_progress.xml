<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="20.0dip" />
            <gradient android:startColor="#ff666666" android:endColor="#ff666666" android:angle="270.0" android:centerY="0.5" android:centerColor="#ff666666" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="20.0dip" />
                <gradient android:startColor="#fff4c42f" android:endColor="#fff4c42f" android:angle="90.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="20.0dip" />
                <gradient android:startColor="#fff4c42f" android:endColor="#fff4c42f" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>