<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#161616"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60sp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/home_bottom_tab_bar_budz_feed_btn"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/icon_feeds"
                android:layout_width="34sp"
                android:layout_height="28sp"
                android:padding="0sp"
                android:src="@drawable/social_wall_new" />

            <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0sp"
                android:gravity="center"
                android:text="Q&amp;A"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:visibility="gone" />
        </LinearLayout>

        <View
            android:layout_width="1sp"
            android:layout_height="match_parent"
            android:layout_marginBottom="6sp"
            android:layout_marginTop="6sp"
            android:background="#5d5d5d" />

        <RelativeLayout
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/icon"
                    android:layout_width="30sp"
                    android:layout_height="28sp"
                    android:padding="2sp"
                    android:src="@drawable/ic_tab_qa" />

                <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0sp"
                    android:gravity="center"
                    android:text="Q&amp;A"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>

            <Button
                android:id="@+id/tab_one"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/transparent"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#6D749E"
                android:textSize="16sp" />

            <View
                android:layout_width="1sp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="6sp"
                android:layout_marginTop="6sp"
                android:background="#5d5d5d" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="44sp"
                    android:layout_height="24sp"
                    android:src="@drawable/ic_tab_group" />

                <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3sp"
                    android:gravity="center"
                    android:text="GROUPS"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>

            <com.codingpixel.healingbudz.customeUI.HealingBudButton
                android:id="@+id/tab_two"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"

                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:background="@color/transparent"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#6D749E"
                android:textSize="16sp" />

            <View
                android:layout_width="1sp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="6sp"
                android:layout_marginTop="6sp"
                android:background="#5d5d5d" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="20sp"
                    android:layout_height="21sp"
                    android:padding="1sp"
                    android:src="@drawable/ic_tab_journal" />

                <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6sp"
                    android:gravity="center"
                    android:text="JOURNALS"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>

            <com.codingpixel.healingbudz.customeUI.HealingBudButton
                android:id="@+id/tab_three"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/transparent"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#6D749E"
                android:textSize="16sp" />

            <View
                android:layout_width="1sp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="6sp"
                android:layout_marginTop="6sp"
                android:background="#5d5d5d" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="24sp"
                    android:layout_height="25sp"
                    android:src="@drawable/ic_tab_strain" />

                <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3sp"
                    android:gravity="center"
                    android:text="STRAINS"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>

            <com.codingpixel.healingbudz.customeUI.HealingBudButton
                android:id="@+id/tab_four"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/transparent"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#6D749E"
                android:textSize="16sp" />

            <View
                android:layout_width="1sp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="6sp"
                android:layout_marginTop="6sp"
                android:background="#5d5d5d" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="25sp"
                    android:layout_height="26sp"
                    android:padding="0sp"
                    android:src="@drawable/budzmapnewic" />

                <com.codingpixel.healingbudz.customeUI.HealingBudTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3sp"
                    android:gravity="center"
                    android:text="BUDZMAP"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>

            <com.codingpixel.healingbudz.customeUI.HealingBudButton
                android:id="@+id/tab_five"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:background="@color/transparent"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#6D749E"
                android:textSize="16sp" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>